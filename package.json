{
  "name": "loplat-ui",
  "version": "1.10.1",
  "description": "A React UI Component Library, powered by loplat Frontend Team.",
  "author": "loplat",
  "license": "MIT",
  "homepage": "https://ui.loplat.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/loplat/loplat-ui"
  },
  "bugs": {
    "url": "https://github.com/loplat/loplat-ui/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "design-system",
    "ui-library",
    "ui",
    "loplat"
  ],
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "files": [
    "esm",
    "assets",
    "components",
    "core",
    "utils",
    "*.js",
    "*.js.map",
    "index.d.ts",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "postpublish": "sh clean-build-files.sh",
    "icon": "cd src/assets/Icon && sh generate-icon.sh",
    "logo": "cd src/assets/Logo && sh generate-logo.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-storybook": "firebase deploy",
    "test-storybook": "test-storybook --stories-json",
    "test": "jest --watch --env=jsdom",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "web-animations-js": "^2.3.2"
  },
  "peerDependencies": {
    "@emotion/css": ">=11.x",
    "@emotion/react": ">=11.x",
    "@emotion/styled": ">=11.x",
    "react": ">=17.x",
    "react-dom": ">=17.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.8",
    "@storybook/jest": "^0.0.9",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/react": "^6.4.19",
    "@storybook/test-runner": "^0.0.4",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.3.8",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": ">=6",
    "jest": "^26.5.3",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^26.5.3",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
